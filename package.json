{
  "name": "node-zookeeper-lock",
  "version": "0.0.1",
  "description": "Implement lock recipe via node-zookeeper-client",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "babel src -d dist",
    "dev": "nodemon --ignore dist/ --exec 'npm run lint && npm run build'",
    "lint": "eslint src/** --ignore-pattern *.spec.js",
    "test": "nodemon test/setup.js src/**/__tests__/*.spec.js --exec 'npm run lint && mocha --compilers js:babel-register' --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GalenWarren/node-zookeeper-lock.git"
  },
  "keywords": [
    "zookeeper",
    "lock"
  ],
  "author": "Galen Warren <galen@cvillewarrens.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GalenWarren/node-zookeeper-lock/issues"
  },
  "homepage": "https://github.com/GalenWarren/node-zookeeper-lock#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  }
}
