{
  "name": "node-zookeeper-recipes",
  "version": "0.0.1",
  "description": "Implement Zookeeper recipes via node-zookeeper-client",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run watch",
    "build": "babel src -d dist",
    "doc": "jsdoc src -r -R README.md -c .jsdoc.json",
    "lint": "eslint -c .eslintrc.json src/**/*.js",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-core/register test/setup.js test/**/*.spec.js",
    "report": "nyc report --reporter=lcov",
    "watch": "nodemon --watch src --watch test --exec 'npm run doc && npm run lint && npm run test && npm run report'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GalenWarren/node-zookeeper-lock.git"
  },
  "keywords": [
    "zookeeper",
    "lock",
    "recipes"
  ],
  "author": "Galen Warren <galen@cvillewarrens.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GalenWarren/node-zookeeper-lock/issues"
  },
  "homepage": "https://github.com/GalenWarren/node-zookeeper-lock#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "node-zookeeper-client": "^0.2.2",
    "pify": "^2.3.0",
    "rxjs": "^5.1.1",
    "uuid": "^3.0.1"
  }
}
